<?xml version="1.0" encoding="utf-8" ?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item
		android:id="@android:id/background"
		android:height="4.0dip"
		android:gravity="fill_horizontal|center">
		<selector>
			<item android:state_enabled="false">
				<shape
					android:shape="rectangle"
					android:tint="?attr/colorControlNormal">
					<corners android:radius="24.0dip" />
					<size android:height="4.0dip" />
					<solid android:color="#33000000" />
				</shape>
			</item>
			<item android:state_enabled="true">
				<shape
					android:shape="rectangle"
					android:tint="?attr/colorPrimaryInverse">
					<corners android:radius="24.0dip" />
					<size android:height="4.0dip" />
					<solid android:color="#4D000000" />
				</shape>
			</item>
		</selector>
	</item>
	<item
		android:id="@android:id/secondaryProgress"
		android:height="4.0dip"
		android:gravity="fill_horizontal|center">
		<scale android:scaleWidth="100%">
			<selector>
				<item android:state_enabled="false">
					<shape
						android:shape="rectangle"
						android:tint="?attr/colorControlNormal">
						<corners android:radius="24.0dip" />
						<size android:height="4.0dip" />
						<solid android:color="#33000000" />
					</shape>
				</item>
				<item android:state_enabled="true">
					<shape
						android:shape="rectangle"
						android:tint="?android:colorControlActivated">
						<corners android:radius="24.0dip" />
						<size android:height="4.0dip" />
						<solid android:color="#59000000" />
					</shape>
				</item>
			</selector>
		</scale>
	</item>
	<item
		android:id="@android:id/progress"
		android:height="4.0dip"
		android:gravity="fill_horizontal|center">
		<scale android:scaleWidth="100%">
			<selector>
				<item android:state_enabled="false">
					<shape
						android:shape="rectangle"
						android:tint="?attr/colorControlNormal">
						<corners android:radius="24.0dip" />
						<size android:height="4.0dip" />
						<solid android:color="#33000000" />
					</shape>
				</item>
				<item android:state_enabled="true">
					<shape
						android:shape="rectangle"
						android:tint="?attr/colorPrimary">
						<corners android:radius="24.0dip" />
						<size android:height="4.0dip" />
						<solid android:color="#F2000000" />
					</shape>
				</item>
			</selector>
		</scale>
	</item>
</layer-list>
